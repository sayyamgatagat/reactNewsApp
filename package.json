{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Starter kit for creating React and Express apps",
  "main": "server.js",
  "scripts": {
    "heroku-postbuild": "cd client && npm install",
    "client-install": "cd client && npm install",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Sayyam Gatagat",
  "license": "V.JS",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "react": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "unirest": "^0.6.0",
    "web-vitals": "^1.1.0"
  },
  "proxy": "http://localhost:5000/"
}
